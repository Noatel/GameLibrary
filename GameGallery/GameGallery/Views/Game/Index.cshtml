@Model String[] Games
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Name
        </th>
        <th>
            Image
        </th>


    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item[0]</td>
        <td>@item[1]</td>
        <td><img src="@item[2]" alt="Alternate Text" /></td>
    </tr>
    }
</table>
<strong style="color:green">@TempData["status"]</strong>
